{
    "private": true,
    "scripts": {
        "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "dev-scss": "npm run development --section=scss",
        "dev-js": "npm run development --section=js",
        "dev": "run-p dev-scss dev-js",
        "original-watch": "npm run development -- --watch",
        "watch-scss": "npm run original-watch --section=scss",
        "watch-js": "npm run original-watch --section=js",
        "watch": "run-p watch-scss watch-js",
        "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "prod-admin-scss": "npm run production --section=scss",
        "prod-admin-js": "npm run production --section=js",
        "prod": "run-p prod-scss prod-js"
    },
    "devDependencies": {
        "axios": "^0.19.2",
        "browser-sync": "^2.26.7",
        "browser-sync-webpack-plugin": "^2.0.1",
        "cross-env": "^7.0",
        "laravel-mix": "^5.0.1",
        "lodash": "^4.17.15",
        "resolve-url-loader": "^3.1.0",
        "sass": "^1.15.2",
        "sass-loader": "^8.0.0",
        "vue-template-compiler": "^2.6.11"
    },
    "dependencies": {
        "@inertiajs/inertia": "^0.1.9",
        "@inertiajs/inertia-vue": "^0.1.4",
        "babel-eslint": "^10.1.0",
        "eslint": "^7.2.0",
        "eslint-loader": "^4.0.2",
        "eslint-plugin-vue": "^6.2.2",
        "laravel-mix-merge-manifest": "^0.1.2",
        "npm-run-all": "^4.1.5",
        "stylelint": "^13.6.1",
        "stylelint-webpack-plugin": "^2.1.0",
        "vue": "^2.6.11",
        "vuelidate": "^0.7.5",
        "vuetify": "^2.3.1",
        "vuex": "^3.4.0"
    }
